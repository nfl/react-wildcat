{
    "name": "react-wildcat-example",
    "description": "An example Wildcat project.",
    "version": "7.4.0",
    "dependencies": {
        "babel-jest": "^20.0.3",
        "koa-favicon": "^1.2.1",
        "koa-route": "^2.4.2",
        "koa-static": "^2.0.0",
        "prop-types": "^15.5.10",
        "radium": "^0.18.1",
        "react-dom": "^15.4.2",
        "react-helmet": "^4.0.0",
        "react-metrics": "^2.2.3",
        "react-router": "^3.0.2",
        "react-wildcat": "7.4.0",
        "react-wildcat-prefetch": "7.4.0",
        "rimraf": "^2.5.4"
    },
    "devDependencies": {
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.10",
        "babel-plugin-dynamic-import-node": "^1.0.2",
        "babel-plugin-flow-runtime": "^0.3.0",
        "babel-plugin-istanbul": "^3.1.2",
        "babel-plugin-module-resolver": "^2.5.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.22.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-inline-environment-variables": "^6.8.0",
        "babel-plugin-transform-object-rest-spread": "^6.22.0",
        "babel-plugin-transform-remove-strict-mode": "^0.0.2",
        "babel-polyfill": "^6.16.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.16.0",
        "babel-preset-react-optimize": "^1.0.1",
        "babel-register": "^6.22.0",
        "chai": "^3.5.0",
        "chai-as-promised": "^6.0.0",
        "circular-dependency-plugin": "^2.0.0",
        "concurrently": "^3.1.0",
        "enzyme": "^2.7.1",
        "eslint": "^3.10.2",
        "eslint-config-nfl": "^11.1.0",
        "eslint-config-prettier": "^2.2.0",
        "eslint-import-resolver-babel-module": "^3.0.0",
        "eslint-plugin-async-await": "^0.0.0",
        "eslint-plugin-flowtype": "^2.25.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-mocha": "^4.7.0",
        "eslint-plugin-prettier": "^2.1.2",
        "eslint-plugin-react": "^6.7.1",
        "event-source-polyfill": "^0.0.9",
        "file-loader": "^0.10.0",
        "flow-runtime": "^0.3.0",
        "fs-extra": "^2.0.0",
        "happypack": "^3.1.0",
        "isomorphic-fetch": "^2.2.1",
        "istanbul": "https://github.com/nfl/istanbul/tarball/0.4.5/source-map-resolve",
        "jest": "^20.0.4",
        "mocha": "^3.2.0",
        "prettier": "^1.4.4",
        "progress-bar-webpack-plugin": "^1.9.3",
        "protractor": "^5.1.0",
        "protractor-console": "^2.0.1",
        "protractor-istanbul-plugin": "^2.0.0",
        "react": "^15.4.2",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-wildcat-test-runners": "7.0.0",
        "replace": "^0.3.0",
        "sinon": "^1.17.6",
        "sinon-chai": "^2.8.0",
        "sw-precache": "^4.2.1",
        "url-loader": "^0.5.7",
        "webpack": "^2.2.1",
        "webpack-hot-middleware": "^2.16.1",
        "webpack-node-externals": "^1.5.4",
        "webpack-visualizer-plugin": "^0.1.10"
    },
    "jest": {
        "collectCoverage": true,
        "coverageDirectory": "coverage/unit",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg)$": "<rootDir>/src/test/mocks/fileMock.js"
        },
        "testRegex": "/src/.*Test\\.js$"
    },
    "scripts": {
        "build:precache": "node ./config/serviceWorkers.js",
        "bundle:bootstrap": "node ./shell/webpack/bundleBootstrap.js",
        "bundle:prod": "env NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config config/webpack/production.config.js",
        "bundle:stats": "env NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config config/webpack/stats.config.js",
        "clean:all": "npm run clean:cache && npm run clean:prod",
        "clean:cache": "rimraf .cache",
        "clean:e2e": "rimraf coverage/e2e",
        "clean:prod": "rimraf bundles public static/serviceWorker.js",
        "clean:unit": "rimraf coverage/unit",
        "dev": "env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 concurrently -k -r 'wildcat' 'wildcat-static-server'",
        "dev:sw": "env SERVICE_WORKERS=true npm run dev",
        "flow": "flow; test $? -eq 0 -o $? -eq 2",
        "lint": "eslint src/* --cache --cache-location .cache/.eslintcache",
        "predev": "npm run build:precache && npm run bundle:bootstrap",
        "preprod": "npm run build:precache && npm run clean:prod && npm run bundle:prod",
        "pretest:e2e": "npm run clean:e2e && env BABEL_ENV=test npm run preprod",
        "pretest:unit": "npm run clean:unit",
        "prod": "env NODE_ENV=production wildcat",
        "prod-local": "env NODE_TLS_REJECT_UNAUTHORIZED=0 concurrently -k -r 'npm run prod' 'npm run prod-static'",
        "prod-local:sw": "env SERVICE_WORKERS=true npm run prod-local",
        "prod-static": "env NODE_ENV=production wildcat-static-server",
        "test": "npm run lint && npm run test:unit && npm run test:e2e",
        "test:e2e": "env BABEL_ENV=test COVERAGE=e2e wildcat-protractor-runner",
        "test:unit": "env BABEL_ENV=test jest",
        "posttest:unit": "node ./src/test/unit/openCoverage.js"
    }
}
