  "description": "Default client/server rendering for react-wildcat",
  "dependencies": {
    "cookie": "^0.3.1",
    "cookies-js": "^1.2.2",
    "history": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "parse-domain": "^1.0.0",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-helmet": "^3.1.0",
    "react-router": "^3.0.0",
    "react-wildcat-hot-reloader": "6.0.0"
